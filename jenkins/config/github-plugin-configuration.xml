<?xml version='1.0' encoding='UTF-8'?>
<github-plugin-configuration plugin="github@1.14.0">
  <configs>
    <github-server-config>
      <apiUrl>https://api.github.com</apiUrl>
      <manageHooks>true</manageHooks>
      <credentialsId>${JENKINS_GITHUB_CREDENTIAL_ID}</credentialsId>
      <clientCacheSize>0</clientCacheSize>
    </github-server-config>
  </configs>
  <hookUrl>${JENKINS_PUBLIC_WEBHOOK_URL}</hookUrl>
</github-plugin-configuration>